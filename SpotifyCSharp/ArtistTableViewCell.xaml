<local:TableViewCell x:Class="SpotifyCSharp.ArtistTableViewCell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SpotifyCSharp"
             mc:Ignorable="d" 
             d:DesignHeight="200" HorizontalAlignment="Stretch" Width="1200" Background="White" MouseLeftButtonDown="ArtistCell_MouseLeftButtonDown">
    <Grid>
        <Image x:Name="ArtistImage" Height="100" Margin="15,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Left" Width="100"/>
        <TextBlock x:Name="ArtistLabel" TextAlignment="Left" Margin="150,0,0,0" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left" Width="660"/>
    </Grid>
</local:TableViewCell>