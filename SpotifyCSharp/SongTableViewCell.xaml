<local:TableViewCell x:Class="SpotifyCSharp.SongTableViewCell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SpotifyCSharp"
             mc:Ignorable="d" 
             d:DesignHeight="200" Background="#FFFFE3E3" Width="900">
    <Grid>
        <Image x:Name="AlbumImage" Height="50" Margin="43,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Left" Width="50"/>
        <TextBlock x:Name="ArtistLabel" Text="Label" HorizontalAlignment="Left" Margin="120,30,0,0" VerticalAlignment="Center"/>
        <TextBlock x:Name="SongLabel" TextAlignment="Left" Margin="120,0,-157,0" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Center" FontWeight="Bold"/>

    </Grid>
</local:TableViewCell>
