<local:TableViewCell x:Class="SpotifyCSharp.SongTableViewCell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SpotifyCSharp"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="200" Background="#FFFFE3E3">
    <Grid>
        <Image x:Name="AlbumImage" Height="100" Margin="50,10,50,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="ArtistLabel" Text="Label" HorizontalAlignment="Center" Margin="0,154,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="SongLabel" TextAlignment="Center" Margin="10,119,10,0" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Bold"/>

    </Grid>
</local:TableViewCell>
